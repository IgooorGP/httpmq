{
  "name": "httpmq",
  "version": "1.0.0",
  "description": "A message broker that works with plain and simple HTTP requestsÔ∏è",
  "main": "app.js",
  "repository": "git@github.com:IgooorGP/httpmq.git",
  "author": "IgooorGP <igor.feq@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "ttsc",
    "start": "typeorm migration:run && node build/src/app.js",
    "build-start": "npm run build && npm run start",
    "dev": "npm run typeorm:migrate && DOTENV_CONFIG_PATH=.local.env ts-node -r tsconfig-paths/register -r dotenv/config src/app.ts",
    "typeorm": "DOTENV_CONFIG_PATH=.local.env node -r ts-node/register -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "npm run typeorm -- migration:run",
    "typeorm:makemigrations": "npm run typeorm -- migration:generate",
    "test": "jest"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.4",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28",
    "winston": "^3.3.3",
    "zod": "^2.0.0-beta.16"
  }
}
