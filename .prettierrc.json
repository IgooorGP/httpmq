{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "quoteProps": "consistent",
  "endOfLine": "lf",
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "insertPragma": false,
  "proseWrap": "always",
  "requirePragma": false
}
